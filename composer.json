{
    "name": "stubbles/webapp-core",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Base classes for Stubbles based web applications.",
    "require": {
        "php": ">=5.6.0",
        "stubbles/core": "^7.0",
        "stubbles/input": "^6.0"
    },
    "suggest": {
        "stubbles/xml": "^5.0",
        "stubbles/image": "^4.0"
    },
    "require-dev": {
        "stubbles/xml": "^5.0",
        "stubbles/image": "^4.0",
        "phpunit/phpunit": "^5.1",
        "mikey179/vfsStream": "^1.6",
        "bovigo/callmap": "^1.1",
        "bovigo/assert": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\webapp\\": "src/main/php"
        },
        "files": ["src/main/php/functions.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
