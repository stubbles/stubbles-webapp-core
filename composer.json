{
    "name": "stubbles/webapp-core",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Base classes for Stubbles based web applications.",
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "stubbles/ioc": "^12.0",
        "stubbles/input": "^10.0",
        "stubbles/peer": "^11.0",
        "stubbles/reflect": "^11.0",
        "stubbles/sequence": "^11.0",
        "stubbles/streams": "^11.0",
        "stubbles/values": "^12.0"
    },
    "suggest": {
        "stubbles/xml": "^10.0",
        "stubbles/image": "^8.0"
    },
    "require-dev": {
        "ext-fileinfo": "*",
        "ext-session": "*",
        "stubbles/xml": "^10.0",
        "stubbles/image": "^8.0",
        "phpunit/phpunit": "^12.5",
        "mikey179/vfsstream": "^1.6.12",
        "bovigo/callmap": "^9.0",
        "bovigo/assert": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\webapp\\": "src/main/php"
        },
        "files": ["src/main/php/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\webapp\\": "src/test/php",
            "stubbles\\helper\\": "src/test/helper"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "11.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
