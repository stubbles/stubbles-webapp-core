{
    "name": "stubbles/webapp-core",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Base classes for Stubbles based web applications.",
    "require": {
        "php": "^7.0",
        "stubbles/ioc": "^8.0",
        "stubbles/input": "^7.0",
        "stubbles/peer": "^8.1",
        "stubbles/reflect": "^8.0",
        "stubbles/sequence": "^8.1",
        "stubbles/streams": "^8.0",
        "stubbles/values": "^8.1"
    },
    "suggest": {
        "stubbles/xml": "^6.0",
        "stubbles/image": "^5.0"
    },
    "require-dev": {
        "stubbles/xml": "^6.0",
        "stubbles/image": "^5.0",
        "phpunit/phpunit": "^8.4",
        "mikey179/vfsstream": "^1.6.7",
        "bovigo/callmap": "^5.1",
        "bovigo/assert": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\webapp\\": "src/main/php"
        },
        "files": ["src/main/php/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\webapp\\": "src/test/php",
            "stubbles\\helper\\": "src/test/helper"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
